    "electron-progressbar": "^2.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.2",
    "extract-zip": "^2.0.1",
    "js-yaml": "^4.1.0",
    "json-to-properties": "^1.1.3",
    "json2yaml": "^1.1.0",
    "mega": "^0.2.0",
    "megajs": "^0.17.2",
    "mineflayer": "^3.11.2",
    "minify": "^8.0.3",
    "ngrok": "^4.2.2",
    "open": "^8.4.0",
    "properties-to-json": "^0.2.1",

  },
  "scripts": {
    "install": "npm install cheerio colors electron electron-is-dev express ngrok socket.io socket.io-client",
    "start": "electron .",
    "start_novsync": "electron . --disable-frame-limit",
    "build": "electron-packager . Minecraft-Custom-Server --overwrite --asar=true --platform=win32 --arch=ia32 --icon=view/data/icons/windows/cumpoopandshit.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Minecraft Custom Server\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "cheerio": "^1.0.0-rc.10",
